{
  "name": "ai-musiktjanst",
  "version": "1.0.0",
  "description": "En svensk musiktjänst för företag med energibaserad taggning",
  "main": "src/server/index.ts",
  "proxy": "http://localhost:3006",
  "scripts": {
    "start": "NODE_ENV=production ts-node src/server/index.ts",
    "dev": "nodemon src/server/index.ts",
    "dev:client": "react-scripts start",
    "build": "tsc && npm run build:client",
    "build:client": "react-scripts build",
    "test": "jest",
    "setup-db": "ts-node src/database/setup.ts",
    "update-db": "ts-node src/database/update_schema.ts",
    "import-songs": "ts-node src/scripts/importSongs.ts",
    "migrate": "ts-node src/database/setup.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.743.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-id3": "^0.2.6",
    "pg": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "stripe": "^12.0.0",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.26",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
